    module: 'roi_data_layer.layer'
    layer: 'RoIDataLayer'
    param_str: "'num_classes': 4"
  }
  transform_param {
    mirror: true
    crop_size: 200

 }
  data_param {   
    batch_size: 100
 
  }
 image_data_param {
    new_height:224
    new_width: 224

  }
}
