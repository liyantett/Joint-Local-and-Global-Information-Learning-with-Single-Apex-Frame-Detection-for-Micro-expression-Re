layer {
  name: "context_roi_pool5"
  type: "ROIPooling"
  bottom: "conv5_3"
  bottom: "secondary_rois"
  top: "context_pool5"
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.0625 # 1/16
  }
}
layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "context_pool5"
  top: "fc6"
  param {
    name: "fc6_w"
    lr_mult: 10
    decay_mult: 1
  }
  param {
    name: "fc6_b"
    lr_mult: 20
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "context_relu6"
  type: "ReLU"
  bottom: "fc6"
  top: "fc6"
}
layer {
  name: "context_drop6"
  type: "Dropout"
  bottom: "fc6"
  top: "fc6"
  dropout_param {
    dropout_ratio: 0.8
  }
}
layer {
  name: "fc7"
  type: "InnerProduct"
  bottom: "fc6"
  top: "fc7"
  param {
    name: "fc7_w"
    lr_mult: 10
    decay_mult: 1
  }
  param {
    name: "fc7_b"
    lr_mult: 20
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "context_relu7"
  type: "ReLU"
  bottom: "fc7"
  top: "fc7"
}
layer {
  name: "context_drop7"
  type: "Dropout"
  bottom: "fc7"
  top: "fc7"
  dropout_param {
    dropout_ratio: 0.8
  }
}
layer {
  name: "context_cls_score"
  type: "InnerProduct"
  bottom: "fc7"
  top: "context_cls_score"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 20
    decay_mult: 0
  }
  inner_product_param {
    num_output: 5
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "MIL_score"
  type: "MIL"
  bottom: "context_cls_score"
  top: "mil_context_cls_score"
  mil_param {
    context_per_roi: 10
  }
}



layer {
  name: "loss_milcls1"
  type: "SoftmaxWithLoss"
  bottom: "mil_context_cls_score"
  bottom: "labels"
  top: "loss_milcls1"

}









